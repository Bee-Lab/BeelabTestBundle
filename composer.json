{
    "name": "beelab/test-bundle",
    "type": "symfony-bundle",
    "description": "A test library for Symfony framework",
    "keywords": ["Symfony", "test"],
    "homepage": "http://bee-lab.net",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": "^2.3|^3.0",
        "doctrine/data-fixtures": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~1.11"
    },
    "autoload": {
        "psr-4": { "Beelab\\TestBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
