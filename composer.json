{
    "name": "beelab/test-bundle",
    "type": "symfony-bundle",
    "description": "A test library for Symfony framework",
    "keywords": ["Symfony", "test"],
    "homepage": "https://github.com/bee-lab",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/data-fixtures": "^1.2",
        "symfony/dependency-injection": "^3.4|^4.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "friendsofphp/php-cs-fixer": "^2.0",
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^6.5",
        "symfony/browser-kit": "^3.4|^4.0",
        "symfony/doctrine-bridge": "^3.4|^4.0",
        "symfony/dom-crawler": "^3.4|^4.0",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/security-core": "^3.4|^4.0",
        "symfony/swiftmailer-bridge": "^3.4|^4.0"
    },
    "autoload": {
        "psr-4": { "Beelab\\TestBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Beelab\\TestBundle\\": "tests/" }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
