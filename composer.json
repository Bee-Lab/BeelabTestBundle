{
    "name": "beelab/test-bundle",
    "type": "symfony-bundle",
    "description": "A test library for Symfony framework",
    "keywords": ["Symfony", "test"],
    "homepage": "https://github.com/bee-lab",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "^2.8|^3.2",
        "doctrine/data-fixtures": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": { "Beelab\\TestBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
